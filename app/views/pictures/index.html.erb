<h1>All Pictures</h1>
<p><%= link_to("Add a new picture", new_picture_url) %></p>

<% @pictures = Picture.all %>
<% @pictures.each do |pic| %>
  <h3> <%= pic.caption %> </h3>
    <%= image_tag(pic.source, :width => 200) %>
    <p></p>
    <ul><%= link_to("Picture Details", picture_url(pic.id) ) %></ul>
    <p></p>
    <p></p>
<% end %>
